{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "airbnb-base"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "import/prefer-default-export": "off",
      "class-methods-use-this": "off",
      "linebreak-style": "off",
      "strict": "off",
      "no-param-reassign": "off",
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "always"
        }
      ]
    }
}
